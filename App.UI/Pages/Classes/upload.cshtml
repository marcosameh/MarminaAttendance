@page
@model App.UI.Pages.Classes.uploadModel
@{
    ViewData["Title"] = "رفع بيانات المخدومين";
}

<div class="container">
    <h1>رفع بيانات المخدومين</h1>
    <form method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="select">الفصل</label>
            <select class="form-control" asp-for="ClassId" id="select" asp-items="Model.ClassesSelectList" required>
                
            </select>
        </div>
        <div class="form-group">
            <label for="file">:قم برفع اكسل شيت</label>
            <input type="file" class="form-control-file" id="file" accept=".xls,.xlsx,.csv" required asp-for="ExcelFile">
        </div>
        <button type="submit" class="btn btn-primary">Upload</button>
    </form>
</div>

@section Scripts{

    <!-- Bootstrap 4 JS (required for some features) -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

}
